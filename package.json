{
  "name": "developer-test-challenge",
  "version": "1.0.4",
  "description": "Developer Test Challenge",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:dev": "ts-node-dev --inspect=0.0.0.0:9229 --watch --clear src/index.ts",
    "test": "PORT=3001 ts-mocha -w --watch-extensions ts --watch-files 'src','tests' 'tests/**/*.spec.{ts,tsx}'",
    "test:nots": "PORT=3001 mocha --require ts-node/register --extensions ts --watch --watch-files 'src','tests' 'tests/**/*.spec.{ts,tsx}'",
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.ts\" --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petervavro/Developer-Test-Challenge"
  },
  "keywords": [],
  "author": "Peter Vavro",
  "license": "ISC",
  "homepage": "https://github.com/petervavro/Developer-Test-Challenge#readme",
  "dependencies": {
    "argon2-pass": "^1.0.2",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "celebrate": "^12.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "pg": "^7.18.2",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.16.1",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^7.0.2",
    "@types/node": "^11.15.9",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "chai": "^4.2.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "npm": "^6.14.4",
    "prettier": "^2.0.2",
    "supertest": "^4.0.2",
    "typescript": "^3.4.3",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
